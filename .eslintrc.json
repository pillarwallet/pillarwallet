{
  "plugins": [
    "react",
    "react-native",
    "jest",
    "flowtype"
  ],
  "env": {
    "node": true,
    "es6": true,
    "react-native/react-native": true,
    "jest/globals": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "class-methods-use-this": [0],
    "react/prop-types": [0],
    "import/no-extraneous-dependencies": ["error", {"packageDir": "./"}]
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb",
    "plugin:flowtype/recommended"
  ]
}
