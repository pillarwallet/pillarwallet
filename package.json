{
  "name": "pillarwallet",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@storybook/addon-actions": "^3.4.1",
    "@storybook/addon-links": "^3.4.1",
    "@storybook/addons": "^3.4.1",
    "@storybook/react-native": "^3.4.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-flowtype-errors": "^3.5.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jest": "^21.15.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-react-native": "^3.2.1",
    "flow-bin": "0.70.0",
    "flow-typed": "^2.4.0",
    "jest": "^22.4.3",
    "jest-expo": "26.0.0",
    "jest-html-reporter": "2.0.0",
    "jest-styled-components": "^5.0.1",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "react-dom": "^16.3.0-alpha.1",
    "react-native-debugger-open": "^0.3.17",
    "react-native-dotenv": "^0.1.1",
    "react-native-scripts": "1.13.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.3.0-alpha.1",
    "redux-mock-store": "^1.5.1",
    "run-s": "^0.0.0"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "flow": "flow",
    "flow-stop": "flow stop",
    "flow-typed": "flow-typed install",
    "validate": "run-s flow test lint",
    "jest": "jest --colors",
    "jest-watch": "jest --colors --watchAll",
    "jest-update": "jest --colors -u",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "postinstall": "rndebugger-open"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "roots": [
      "."
    ],
    "testRegex": "\\.test\\.js$",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/reports/**",
      "!**/flow-typed/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./reports",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-vector-icons|react-native-keyboard-aware-scroll-view|react-navigation|react-native-safe-area-view|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react|pouchdb-react-native|react-navigation-redux-helpers|pouchdb-core|expo|pouchdb-adapter-asyncstorage|react-native-maps|react-native-iphone-x-helper|tcomb-form-native)/)"
    ],
    "setupTestFrameworkScriptFile": "./src/testUtils/jestSetup.js",
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Jest results",
    "outputPath": "./reports/jest/test-report.html",
    "includeFailureMsg": true
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "ethers": "^3.0.9",
    "expo": "^26.0.0",
    "lodash.merge": "^4.6.1",
    "native-base": "^2.4.2",
    "polished": "^1.9.2",
    "pouchdb-react-native": "^6.4.1",
    "qrcode": "^1.2.0",
    "react": "^16.3.0-alpha.1",
    "react-native": "^0.54.2",
    "react-native-canvas": "^0.1.20",
    "react-navigation": "^1.5.11",
    "react-navigation-redux-helpers": "^1.0.5",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "shuffle-array": "^1.0.1",
    "styled-components": "^3.2.5",
    "tcomb-form-native": "^0.6.11"
  }
}
