  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "flow": "flow",
    "flow-stop": "flow stop",
    "flow-typed": "flow-typed install",
    "validate": "run-s flow test lint",
    "jest": "jest --colors",
    "jest-watch": "jest --colors --watchAll",
    "jest-update": "jest --colors -u",
    "prestorybook": "rnstl",
    "storybook": "storybook start -p 7007",
    "postinstall": "./node_modules/.bin/rn-nodeify --install --hack --yarn && mv shim.js src/utils && rndebugger-open"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./src"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "roots": [
      "."
    ],
    "testRegex": "\\.test\\.js$",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/reports/**",
      "!**/flow-typed/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./reports",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-modal|react-native-communications|react-native-qrcode|react-native-gifted-chat|react-native-parsed-text|@expo/react-native-action-sheet|@expo/vector-icons|react-native-intercom|react-native-firebase|react-native-animatable|react-native-vector-icons|react-native-invertible-scroll-view|react-native-scrollable-mixin|react-native-device-info|react-native-lightbox|react-native-keyboard-aware-scroll-view|react-navigation|react-native-safe-area-view|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-scrypt|react|pouchdb-react-native|react-navigation-redux-helpers|pouchdb-core|expo|pouchdb-adapter-asyncstorage|react-native-maps|react-native-iphone-x-helper|tcomb-form-native|react-navigation-tabs|native-base|react-navigation-fluid-transitions)/)"
    ],
    "setupTestFrameworkScriptFile": "./src/testUtils/jestSetup.js",
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Jest results",
    "outputPath": "./reports/jest/test-report.html",
    "includeFailureMsg": true
  },
  "dependencies": {
    "@expo/vector-icons": "^6.3.1",
    "@pillarwallet/pillarwallet-nodejs-sdk": "^1.4.622",
    "@tradle/react-native-http": "^2.0.0",
