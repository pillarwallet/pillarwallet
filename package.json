    "preset": "react-native",
    "verbose": true,
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "\\.test\\.js$",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/reports/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./reports",
    "setupFilesAfterEnv": [
      "./src/testUtils/jestSetup.js"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-[a-z, -]*|react-native-[a-z, -]*|native-base-[a-z, -]*|react-native|native-base|pouchdb-react-native|@react-navigation|pouchdb-adapter-asyncstorage|tcomb-form-native|concat-color-matrices|rn-color-matrices|@react-native-firebase|@expo/react-native-action-sheet)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/mocks/fileMock.js"
    }
  },
  "jest-html-reporter": {
