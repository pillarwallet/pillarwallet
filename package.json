  "scripts": {
    "start": "react-native start --reset-cache",
    "android": "react-native run-android --variant=devDebug",
    "ios": "react-native run-ios",
    "generate:ios:ipa": "mkdir  ./ios/build/Build/Products/Release-iphoneos/Payload && cp -R ./ios/build/Build/Products/Release-iphoneos/pillarwallet.app ./ios/build/Build/Products/Release-iphoneos/Payload && cd ./ios/build/Build/Products/Release-iphoneos/ && /usr/bin/zip -r  pillarwallet.ipa Payload && rm -rf Payload && mv pillarwallet.ipa ../../../..",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/pillarwallet/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "flow": "flow",
    "flow-stop": "flow stop",
    "flow-typed": "flow-typed install",
    "validate": "run-s flow test lint",
    "jest": "jest --colors",
    "jest-watch": "jest --colors --watchAll",
    "jest-update": "jest --colors -u",
    "postinstall": "./node_modules/.bin/rn-nodeify --install --hack --yarn && mv shim.js src/utils && rndebugger-open",
    "precommit": "yarn run validate"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    },
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "testRegex": "\\.test\\.js$",
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/reports/**",
      "!**/flow-typed/**"
    ],
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./reports",
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|react-native-modal|react-native-cached-image|concat-color-matrices|rn-color-matrices|react-native-color-matrix-image-filters|react-native-shadow|react-native-background-timer|react-native-share|react-native-communications|react-native-qrcode|react-native-gifted-chat|react-native-svg|@expo/react-native-action-sheet|react-native-parsed-text|react-native-camera|react-native-intercom|react-native-firebase|react-native-animatable|react-native-vector-icons|react-native-linear-gradient|react-native-invertible-scroll-view|react-native-scrollable-mixin|react-native-device-info|react-native-lightbox|react-native-keyboard-aware-scroll-view|react-navigation|react-native-safe-area-view|native-base-shoutem-theme|react-native-easy-grid|react-native-drawer|react-native-scrypt|react|pouchdb-react-native|react-navigation-redux-helpers|pouchdb-core|pouchdb-adapter-asyncstorage|react-native-maps|react-native-iphone-x-helper|tcomb-form-native|react-navigation-tabs|native-base|react-native-sentry|react-native-image-crop-picker|react-navigation-fluid-transitions)/)"
    ],
    "setupTestFrameworkScriptFile": "./src/testUtils/jestSetup.js",
    "moduleNameMapper": {
      "^image![a-zA-Z0-9$_-]+$": "GlobalImageStub",
      "^[@./a-zA-Z0-9$_-]+\\.(png|gif)$": "RelativeImageStub"
    }
  },
  "jest-html-reporter": {
    "pageTitle": "Jest results",
    "outputPath": "./reports/jest/test-report.html",
    "includeFailureMsg": true
  },
